{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "hubName": "%TaskHub%"
    }
  },
  "logging": {
    "logLevel": {"default": "Debug"},
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[2.*, 3.0.0)"
  }
}
